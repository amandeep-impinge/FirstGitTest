<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
android:background="@color/white"
  >
  
  <TextView 
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
 android:text="setup"
android:textColor="@color/black"
 android:layout_marginTop="10dp" 
 android:textSize="20dip"
android:layout_gravity="center_horizontal" 

/>
<ScrollView 
 android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_marginTop="25dp"
>
<LinearLayout 
android:orientation="vertical"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
>
<RelativeLayout 
android:layout_width="wrap_content"
  android:layout_height="wrap_content"
android:padding="10dp"
>

<TextView 
android:layout_width="wrap_content"
  android:layout_height="wrap_content"
android:text="Facebook"
android:id="@+id/facebook"
android:textColor="@color/black"

/>

<Button 
android:layout_width="wrap_content"
  android:layout_height="wrap_content"
android:layout_alignParentRight="true"


android:text="sign in >"
/>

<TextView 

android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:id="@+id/location"
android:layout_below="@+id/facebook"
android:text="Location service"
android:layout_marginTop="30dp"
android:textColor="@color/black"
/>
<Button 
android:layout_width="wrap_content"
  android:layout_height="wrap_content"
android:layout_alignParentRight="true"
android:layout_below="@+id/facebook"
android:layout_marginTop="30dp"
android:text="Sign in >"

/>
<TextView 

android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:id="@+id/location_sevice"
android:layout_below="@+id/location"
android:text="Location "
android:layout_marginTop="35dp"
android:textColor="@color/black"
/>


<TextView 
android:layout_width="wrap_content"
  android:layout_height="wrap_content"
android:layout_alignParentRight="true"
android:layout_below="@+id/location"
android:layout_marginTop="35dp"
android:text="Amststerdam >"
android:textColor="@color/black"
/>

<TextView 

android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:id="@+id/language"
android:layout_below="@+id/location_sevice"
android:text="Language"
android:layout_marginTop="35dp"
android:textColor="@color/black"
/>


<TextView 
android:layout_width="wrap_content"
  android:layout_height="wrap_content"
android:layout_alignParentRight="true"
android:layout_below="@+id/location_sevice"
android:layout_marginTop="35dp"
android:text="English >"
android:textColor="@color/black"
/>

<TextView 

android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:id="@+id/setting4"
android:layout_below="@+id/language"
android:text="Setting 4"
android:layout_marginTop="35dp"
android:textColor="@color/black"
/>


<TextView 
android:layout_width="wrap_content"
  android:layout_height="wrap_content"
android:layout_alignParentRight="true"
android:layout_below="@+id/language"
android:layout_marginTop="35dp"
android:text="Option 4 >"
android:textColor="@color/black"
/>

<TextView 

android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:id="@+id/setting5"
android:layout_below="@+id/setting4"
android:text="Setting 5"
android:layout_marginTop="35dp"
android:textColor="@color/black"
/>


<TextView 
android:layout_width="wrap_content"
  android:layout_height="wrap_content"
android:layout_alignParentRight="true"
android:layout_below="@+id/setting4"
android:layout_marginTop="35dp"
android:text="Option 5 >"
android:textColor="@color/black"
/>


<TextView 

android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:id="@+id/setting6"
android:layout_below="@+id/setting5"
android:text="Setting 6"
android:layout_marginTop="35dp"
android:textColor="@color/black"
/>


<TextView 
android:layout_width="wrap_content"
  android:layout_height="wrap_content"
android:layout_alignParentRight="true"
android:layout_below="@+id/setting5"
android:layout_marginTop="35dp"
android:text="Option 6 >"
android:textColor="@color/black"
/>

<TextView 

android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:id="@+id/rest"
android:layout_below="@+id/setting6"
android:text="Rest"
android:layout_marginTop="35dp"
android:textColor="@color/black"
/>


<TextView 
android:layout_width="wrap_content"
  android:layout_height="wrap_content"
android:layout_alignParentRight="true"
android:layout_below="@+id/setting6"
android:layout_marginTop="35dp"
android:text=">"
android:textColor="@color/black"
/>




</RelativeLayout>



</LinearLayout>

</ScrollView>

</LinearLayout>
